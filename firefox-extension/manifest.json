{
  "manifest_version": 3,
  "name": "Side Notes",
  "version": "1.0",
  "description": "A simple draggable todo list inside Firefox's sidebar.",
  "sidebar_action": {
    "default_title": "Side Notes",
    "default_panel": "sidebar.html"
  },
  "action": {
    "default_icon": "icon48.png",
    "default_title": "Open Side Notes"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": []
}
